<%- include("../templates/encabezado.ejs")%>
<div class="container mt-5">
    <form action="/nuevousuario" method="post" enctype="multipart/form-data">
        <div class="card">
            <div class="card-header">
                <h1>Bienvenido</h1>
            </div>
            <div class="card-body">
                <input class="form-control mt-3" type="text" name="nombre" id="nombre" placeholder="Nombre" autofocus required>
                <input class="form-control mt-3" type="text" name="usuario" id="usuario" placeholder="Usuario" required>
                <input class="form-control mt-3" type="text" name="password" id="password" placeholder="Password" const packageName = require('packageName');>
                <input class="form-control mt-3" type="file" name="foto" id="foto" required>
            
            </div>
            <div class="card-footer">
               <button class="btn btn-dark d-block w-100">Registrarse</button>
    
            </div>
        </div>
    </form>
</div>

